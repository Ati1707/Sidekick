@if (Filters.Count > 0)
{
    foreach (var filter in Filters)
    {
        <PropertyFilterComponent Filter="filter" />
    }

    <ItemSeparator Rarity="Rarity" />
}

@code {
    [CascadingParameter]
    public List<PropertyFilter> Filters { get; set; }

    [Parameter]
    public Rarity Rarity { get; set; }
}
