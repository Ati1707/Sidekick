
@if (Filters.Count > 0)
{
        foreach (var filter in Filters)
        {
            <PropertyFilterComponent Filter="filter" />
        }

        <ItemSeparatorComponent Rarity="Rarity" />
}

@code {
    [Parameter] public List<PropertyFilter> Filters { get; set; }

    [Parameter] public Rarity Rarity { get; set; }
}
