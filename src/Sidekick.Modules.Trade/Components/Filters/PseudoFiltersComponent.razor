@if (Filters.PseudoFilters.Any())
{
    <ItemSeparator Rarity="Rarity" />
}

@foreach (var filter in Filters.PseudoFilters)
{
    <CascadingValue Value="filter" IsFixed="true">
        <PseudoFilterComponent />
    </CascadingValue>
}

@code {
    [CascadingParameter]
    public FiltersModel Filters { get; set; }

    [Parameter]
    public Rarity Rarity { get; set; }
}
